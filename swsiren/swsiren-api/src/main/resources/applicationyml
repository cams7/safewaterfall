      enabled: true
      web-allow-others: true# Datasource  datasource:    url: ${spring.datasource.url}    username: ${spring.datasource.username}    password: ${spring.datasource.password}    driver-class-name: ${spring.datasource.driver-class-name}    
  jpa:    hibernate:      ddl-auto: ${spring.jpa.hibernate.ddl-auto}    show-sql: ${spring.jpa.properties.hibernate.show_sql}   quartz:    job-store-type: jdbc    jdbc:# Always create the Quartz database on startup      initialize-schema: always
  main:    banner-mode: console    allow-bean-definition-overriding: true

